{
  "model_type": "mom",
  "mom_backend": "gated_deltanet",
  "attn_mode": "chunk",
  
  "hidden_size": 1024,
  "num_hidden_layers": 24,
  "num_heads": 4,
  "head_dim": 256,
  "expand_v": 1,
  "hidden_ratio": 3,
  "intermediate_size": null,
  
  "num_memories": 4,
  "topk": 2,
  "capacity": 1.0,
  "shared_mem": true,
  "single_kv_proj": false,
  
  "use_short_conv": true,
  "conv_size": 4,
  "use_output_gate": true,
  "use_layer_wise_balance": true,
  "aux_loss_scale": 0.01,
  
  "hidden_act": "swish",
  "fuse_cross_entropy": true,
  "norm_eps": 1e-06,
  "initializer_range": 0.02,
  "tie_word_embeddings": true,
  "use_cache": true,
  "vocab_size": 32000,
  "bos_token_id": 1,
  "eos_token_id": 2
}